# Configurações do Ambiente - CompareJa App

# Android SDK
ANDROID_HOME=/opt/android-sdk
ANDROID_SDK_ROOT=/opt/android-sdk

# Firebase (para deploy)
FIREBASE_PROJECT_ID=compareja-app
FIREBASE_APP_ID_STAGING=1:123456789:android:abcdef123456
FIREBASE_APP_ID_PRODUCTION=1:123456789:android:fedcba654321

# Build Configuration
ANDROID_COMPILE_SDK=34
ANDROID_BUILD_TOOLS=34.0.0
ANDROID_MIN_SDK=24
ANDROID_TARGET_SDK=34

# App Configuration
APP_NAME=CompareJa
APP_VERSION=1.0.0
APP_PACKAGE=br.com.fiap.compareja

# Docker Configuration
DOCKER_IMAGE_NAME=compareja-android-builder
DOCKER_TAG=latest

# CI/CD Configuration
GITHUB_REPOSITORY=fiap-esg-app
GITHUB_BRANCH_MAIN=main
GITHUB_BRANCH_DEVELOP=develop
